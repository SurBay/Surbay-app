<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="TabButtonNotSelect">
        <item name="android:background">@drawable/ic_tabnoselect</item>
        <item name="android:textColor">#BDBDBD</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:paddingHorizontal" tools:targetApi="o">5dp</item>
    </style>
    <style name="TabButtonSelect">
        <item name="android:background">@drawable/ic_tabselect</item>
        <item name="android:textColor">#3AD1BF</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:paddingHorizontal" tools:targetApi="o">5dp</item>
    </style>
    <style name="TabButtonNotSelect_free">
        <item name="android:background">@drawable/ic_tabnoselect</item>
        <item name="android:textColor">#BDBDBD</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:paddingHorizontal" tools:targetApi="o">5dp</item>
    </style>
    <style name="TabButtonSelect_free">
        <item name="android:background">@drawable/ic_tabselect</item>
        <item name="android:textColor">@color/free_theme_color</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:paddingHorizontal" tools:targetApi="o">5dp</item>
    </style>
    <style name="tabLayout_free" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">17dp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="Apptheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/teal_200</item>
        <item name="title">SurBay</item>
    </style>
    <style name="mypage_profile">
        <item name="android:background">@drawable/round_border_white</item>
        <item name="backgroundColor">#FFFFFF</item>
    </style>
    <style name="round_border_gray_fill_10px">
        <item name="android:background">@drawable/round_border_gray_fill_10px</item>
    </style>
    <style name="round_border_white_fill_30px">
        <item name="android:background">@drawable/round_border_white_fill_30px</item>
    </style>
    <style name="round_border_white_fill_20px">
        <item name="android:background">@drawable/round_border_white_fill_20px</item>
    </style>
    <style name="search_back">
        <item name="android:background">@drawable/search_back</item>
        <item name="backgroundColor">#FFFFFF</item>
    </style>
    <style name="sex_box_nosel">
        <item name="android:background">@drawable/box_notfill</item>
        <item name="android:textColor">#ACACAC</item>
    </style>
    <style name="sex_box_sel">
        <item name="android:background">@drawable/box_fill</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/design_default_color_primary</item>
        <item name="colorPrimaryDark">@color/design_default_color_primary_dark</item>
        <item name="colorAccent">@color/design_default_color_surface</item>

        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="content_indicator">
        <item name="background">@drawable/content_indicator</item>
    </style>
    <style name="CustomDialog" parent="@android:style/Theme.Translucent.NoTitleBar">
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:padding">0dp</item>
        <item name="android:background">#00000000</item>
    </style>
    <style name="Matisse.White" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/black</item>
        <item name="toolbar">@style/Toolbar.Zhihu</item>
        <item name="album.dropdown.title.color">@color/zhihu_album_dropdown_title_text</item>
        <item name="album.dropdown.count.color">@color/black</item>
        <item name="album.element.color">@android:color/black</item>
        <item name="album.thumbnail.placeholder">@color/zhihu_album_dropdown_thumbnail_placeholder</item>
        <item name="album.emptyView">@drawable/ic_empty_zhihu</item>
        <item name="album.emptyView.textColor">@color/zhihu_album_empty_view</item>
        <item name="item.placeholder">@color/zhihu_item_placeholder</item>
        <item name="item.checkCircle.backgroundColor">@color/zhihu_item_checkCircle_backgroundColor</item>
        <item name="item.checkCircle.borderColor">@color/white</item>
        <item name="page.bg">@color/zhihu_page_bg</item>
        <item name="bottomToolbar.bg">@color/zhihu_bottom_toolbar_bg</item>
        <item name="bottomToolbar.preview.textColor">@color/zhihu_bottom_toolbar_preview</item>
        <item name="bottomToolbar.apply.textColor">@color/zhihu_bottom_toolbar_apply</item>
        <item name="preview.bottomToolbar.back.textColor">@color/zhihu_preview_bottom_toolbar_back_text</item>
        <item name="preview.bottomToolbar.apply.textColor">@color/zhihu_preview_bottom_toolbar_apply</item>
        <item name="listPopupWindowStyle">@style/Popup.Zhihu</item>
        <item name="capture.textColor">@color/zhihu_capture</item>
    </style>
    <style name="ImagePickerTheme" parent="ef_BaseTheme">
        <item name="colorPrimary">#607D8B</item>
        <item name="colorPrimaryDark">#455A64</item>
        <item name="colorAccent">@color/ef_colorAccent</item>
    </style>
    <style name="EdittextTheme" parent="Widget.AppCompat.EditText">
        <item name="android:paddingHorizontal">12.5dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">8.75dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColorHint">@color/nav_gray</item>
        <item name="android:textColor">@color/text_black</item>
        <item name="android:lines">1</item>
    </style>
    <style name="TextviewTheme" parent="Widget.AppCompat.TextView">
        <item name="android:paddingHorizontal">12.5dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">8.75dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColorHint">@color/nav_gray</item>
        <item name="android:textColor">@color/text_black</item>
        <item name="android:lines">1</item>
    </style>
    <style name="textsize" parent="Widget.AppCompat.TextView">
        <item name="android:textSize">15sp</item>
    </style>
    <style name="titleText" parent="Widget.AppCompat.TextView">
        <item name="android:textColor">@color/text_black</item>
        <item name="android:textSize">17.5dp</item>
        <item name="android:layout_marginTop">27dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="round">
        <item name="android:src">@drawable/round</item>
    </style>
    <style name="back_button">
        <item name="android:src">@drawable/leftarrow2</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_marginTop">28dp</item>
        <item name="android:clickable">true</item>
        <item name="android:scaleType">fitCenter</item>
    </style>

</resources>