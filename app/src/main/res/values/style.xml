    <style name="TabButtonNotSelect">
        <item name="android:background">@drawable/ic_tabnoselect</item>
        <item name="android:textColor">#BDBDBD</item>
        <item name="android:textSize">10dp</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:paddingHorizontal" tools:targetApi="o">5dp</item>
    </style>
    <style name="TabButtonSelect">
        <item name="android:background">@drawable/ic_tabselect</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">10dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:paddingHorizontal" tools:targetApi="o">5dp</item>
    </style>
    <style name="Apptheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/teal_200</item>
        <item name="title">SurBay</item>
    </style>
    <style name="mypage_profile">
        <item name="android:background">@drawable/round_border_white</item>
        <item name="backgroundColor">#FFFFFF</item>
